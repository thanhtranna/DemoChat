<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Demo Chat</title>
  <script type="text/javascript" src="/socket.io/socket.io.js"></script>
  <script type="text/javascript" src="jquery.min.js"></script>
  <script type="text/javascript" src="xuly.js"></script>
  <link rel="stylesheet" href="layout.css">
</head>

<body>

  <div id="wrapper">
    <div id="loginForm">
      <h3>What's your name?</h3>
      <input type="text" id="txtUsername" />
      <input type="button" id="btnRegister" value="Register">
    </div>
    <div id="chatForm">
      <div id="left">
        <div id="boxTitle">Users Online</div>
        <div id="boxContent">
          
        </div>
      </div>
      <div id="right">
        <div id="sayHi">
          Hello <span id="currentUser">Teo</span>
          <input type="button" id="btnLogout" value="Logout">
        </div>
        <div id="listMessage">
        </div>
        <!-- end #listMessage -->
        <div id="notification"></div>
        <input type="text" id="txtMessage" required/>
        <input type="button" id="btnSendMessage" value="Send" />

      </div>
    </div>
  </div>

</body>

</html>
